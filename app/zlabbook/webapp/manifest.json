{
    "_version": "1.32.0",
    "sap.app": {
        "id": "zlab.zlabbook.v2",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "dataSources": {
            "mainService": {
                "uri": "/library/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            }
        },
        "offline": false,
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "ui5template.fiorielements.v4.lrop",
            "version": "1.0.0"
        },
        "crossNavigation": {
            "inbounds": {
                "zlab-zlabbook-inbound": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "Booksv2",
                    "action": "manage",
                    "title": "{{flpTitle}}",
                    "subTitle": "{{flpSubtitle}}",
                    "icon": "sap-icon://education",
                    "indicatorDataSource": {
                        "dataSource": "mainService",
                        "path": "/dynamic_dest/zlablibrary-v2-app-srv/library/Books/$count",
                        "refresh": 10
                    }
                }
            },
            "outbounds": {
                "zlab-zlabbook-outbound-authors": {
                    "semanticObject": "Authorsv2",
                    "action": "manage",
                    "parameters": {
                        "toAuthor_authorUUID": {}
                    }
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "resources": {
            "js": [],
            "css": []
        },
        "dependencies": {
            "minUI5Version": "1.97.2",
            "libs": {
                "sap.ui.core": {},
                "sap.fe.templates": {}
            }
        },
        "models": {
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true
                }
            }
        },
        "routing": {
            "routes": [
                {
                    "pattern": ":?query:",
                    "name": "BooksList",
                    "target": "BooksList"
                },
                {
                    "pattern": "Books({key}):?query:",
                    "name": "BooksObjectPage",
                    "target": "BooksObjectPage"
                }
            ],
            "targets": {
                "BooksList": {
                    "type": "Component",
                    "id": "BooksList",
                    "name": "sap.fe.templates.ListReport",
                    "options": {
                        "settings": {
                            "entitySet": "Books",
                            "variantManagement": "Page",
                            "navigation": {
                                "Books": {
                                    "detail": {
                                        "route": "BooksObjectPage"
                                    }
                                }
                            },
                            "initialLoad": "Enabled"
                        }
                    }
                },
                "BooksObjectPage": {
                    "type": "Component",
                    "id": "BooksObjectPage",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "editableHeaderContent": false,
                            "entitySet": "Books",
                            "content": {
                                "header": {
                                    "facets": {
                                        "HeaderCopiesFacet": {
                                            "template": "zlab.zlabbook.v2.custom.HeaderCopiesSection",
                                            "title": "{i18n>copies}",
                                            "position": {
                                                "placement": "After",
                                                "anchor": "HeaderFacetImage"
                                            },
                                            "flexSettings": {
                                                "designtime": "not-adaptable-visibility"
                                            }
                                        },
                                        "HeaderPagesFacet": {
                                            "template": "zlab.zlabbook.v2.custom.HeaderPagesSection",
                                            "title": "{i18n>pages}",
                                            "position": {
                                                "placement": "After",
                                                "anchor": "HeaderCopiesFacet"
                                            },
                                            "flexSettings": {
                                                "designtime": "not-adaptable-visibility"
                                            }
                                        },
                                        "HeaderStatusFacet": {
                                            "template": "zlab.zlabbook.v2.custom.HeaderStatusSection",
                                            "title": "{i18n>bookstatus}",
                                            "position": {
                                                "placement": "After",
                                                "anchor": "HeaderPagesFacet"
                                            },
                                            "flexSettings": {
                                                "designtime": "not-adaptable-visibility"
                                            }
                                        }
                                    }
                                },
                                "body": {
                                    "sections": {
                                        "DetailsSection": {
                                            "title": "{i18n>AdditionalInfo}",
                                            "position": {
                                                "anchor": "PODetails",
                                                "placement": "After"
                                            },
                                            "type": "XMLFragment",
                                            "subSections": {
												"CopyQtySubSection": {
                                                    "template": "zlab.zlabbook.v2.custom.CopyQtySection",
                                                    "type": "XMLFragment",
                                                    "title": "{i18n>copies}"
                                                },
                                                "PriceSubSection": {
                                                    "template": "zlab.zlabbook.v2.custom.Price3Section",
                                                    "type": "XMLFragment",
                                                    "title": "{i18n>price}"
                                                }
											}
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        }
    },
    "sap.platform.abap": {
        "_version": "1.1.0",
        "uri": ""
    },
    "sap.platform.hcp": {
        "_version": "1.1.0",
        "uri": ""
    },
    "sap.fiori": {
        "_version": "1.1.0",
        "registrationIds": [],
        "archeType": "transactional"
    },
    "sap.cloud": {
        "public": true,
        "service": "zlablibrary.v2"
    }
}